{
  "extends": "airbnb-base",
  "parserOptions": {
    "sourceType": "script"
  },
  "env": {
    "node": true,
    "browser": true
  },
  "rules": {
    "max-len": [
      "error",
      200
    ],
    "valid-jsdoc": [
      "error",
      {
        "requireParamDescription": false,
        "requireReturnDescription": false,
        "requireReturn": false,
        "prefer": {
          "return": "returns"
        },
        "preferType": {
          "boolean": "Boolean",
          "number": "Number",
          "object": "Object",
          "function": "Function",
          "string": "String"
        }
      }
    ]
  }
}
